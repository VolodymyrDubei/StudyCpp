#include <iostream>

using namespace std;

int main()
{
	int SomeArray[5][2] = {{0, 0}, {1, 2}, {2, 4}, {3, 6}, {4, 8}};
	for (int i = 0; i < 5; i++)
    	for (int j = 0; j < 2; j++)
    	{
        	cout << "SomeAray[" << i << "][" << j << "]: ";
        	cout << SomeArray[i][j] << endl;
    	}
	return 0;
}
////
SomeAray[0][0]: 0
SomeAray[0][1]: 0
SomeAray[1][0]: 1
SomeAray[1][1]: 2
SomeAray[2][0]: 2
SomeAray[2][1]: 4
SomeAray[3][0]: 3
SomeAray[3][1]: 6
SomeAray[4][0]: 4
SomeAray[4][1]: 8

