#include <iostream>

using namespace std;

int main()
{
    int myInt;
    long myLong;
    double myDouble;
    float myFloat;
    unsigned int myUnsigned;
    char myWord[50];

    cout << "int: ";
    cin >> myInt;
    cout << "Long: ";
    cin >> myLong;
    cout << "Double: ";
    cin >> myDouble;
    cout << "Float: ";
    cin >> myFloat;
    cout << "Word: ";
    cin >> myWord;
    cout << "Unsigned: ";
    cin >> myUnsigned;

    cout << "\n\nInt:\t\t" << myInt << endl;
    cout << "Long:\t\t" << myLong << endl;
    cout << "Double:\t\t" << myDouble << endl;
    cout << "Float:\t\t" << myFloat << endl;
    cout << "Word:\t\t" << myWord << endl;
    cout << "Unsigned:\t" << myUnsigned << endl;
    cout << "\n\nInt, Long, Double, Float, Word, Unsigned: ";
    cin >> myInt >> myLong >> myDouble;
    cin >> myFloat >> myWord >> myUnsigned;
    cout << "\n\nInt:\t\t" << myInt << endl;
    cout << "Long:\t\t" << myLong << endl;
    cout << "Double:\t\t " << myDouble << endl;
    cout << "Float:\t\t" << myFloat << endl;
    cout << "Word:\t\t" << myWord << endl;
    cout << "Unsigned:\t" << myUnsigned << endl;

    return 0;
}
///////////
int: 2
Long: 30303
Double: 393939397834
Float: 3.33
Word: Hello
Unsigned: 85


