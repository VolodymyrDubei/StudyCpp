#include <iostream>
 using namespace std;
 int main()
 {
  	unsigned short int Width = 5, Length;
  	Length = 10;
 
  	// створюємо змінну Area типу unsigned short та ініціалізуємо її
 	// результатом множення значень змінних Width на Length
 	unsigned short int Area = (Width * Length);

 	cout << "Width:" << Width << "\n";
 	cout << "Length:" << Length << endl;
 	cout << "Area: " << Area << endl;
 	return 0;
 }
////
Width:5
Length:10
Area: 50

